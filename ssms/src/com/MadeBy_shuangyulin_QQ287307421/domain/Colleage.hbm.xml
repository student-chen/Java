<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.MadeBy_shuangyulin_QQ287307421.domain">

	<class name="Colleage" table="t_Colleage">

          <!-- 主键学院编号 -->
          <id name="colleageId" column="colleageId">
            <generator class="native"/>
          </id>

          <!-- 简单字符串属性: 学院名称 -->
          <property
            name="colleageName"
            column="colleageName"
            type="string"
            length="20"
          />


		 
		
	</class>
	
</hibernate-mapping>
 
